{
  "name": "portfolio-website",
  "version": "1.0.0",
  "description": "With all of the projects you will be working on here at Lambda School, you will need somewhere to house them!  There is no better opportunity to learn and show off your skills than a beautiful portfolio website.",
  "main": "index.html",
  "scripts": {
    "compile:sass": "node-sass assets/sass/main.scss assets/css/main.css -w",
    "live-server": "live-server",
    "start": "concurrently --names \"sass-compiler,live-server\" \"npm run compile:sass\" \"live-server\" -k"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WilliamConnatser/portfolio-website.git"
  },
  "author": "William Connatser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WilliamConnatser/portfolio-website/issues"
  },
  "homepage": "https://github.com/WilliamConnatser/portfolio-website#readme",
  "dependencies": {
    "concurrently": "^4.1.0"
  },
  "devDependencies": {
    "node-sass": "^4.11.0"
  }
}
